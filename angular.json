{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "HDSDashboardUI": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/HDSDashboardUI",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "./node_modules/@epsilon/core-ui/assets/cui/styles/style.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "HDSDashboardUI:build:development",
            "proxyConfig": "proxy.config.js",
            "disableHostCheck": true
          },
          "configurations": {
            "production": {
              "browserTarget": "HDSDashboardUI:build:production"
            },
            "development": {
              "browserTarget": "HDSDashboardUI:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "HDSDashboardUI:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "common-lib": {
      "projectType": "library",
      "root": "projects/common-lib",
      "sourceRoot": "projects/common-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/common-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/common-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/common-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/common-lib/src/test.ts",
            "tsConfig": "projects/common-lib/tsconfig.spec.json",
            "karmaConfig": "projects/common-lib/karma.conf.js"
          }
        }
      }
    },
    "http-lib": {
      "projectType": "library",
      "root": "projects/http-lib",
      "sourceRoot": "projects/http-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/http-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/http-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/http-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/http-lib/src/test.ts",
            "tsConfig": "projects/http-lib/tsconfig.spec.json",
            "karmaConfig": "projects/http-lib/karma.conf.js"
          }
        }
      }
    },
    "core-lib": {
      "projectType": "library",
      "root": "projects/core-lib",
      "sourceRoot": "projects/core-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/core-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/core-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/core-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/core-lib/src/test.ts",
            "tsConfig": "projects/core-lib/tsconfig.spec.json",
            "karmaConfig": "projects/core-lib/karma.conf.js"
          }
        }
      }
    },
    "deployments-ui": {
      "projectType": "library",
      "root": "projects/deployments-ui",
      "sourceRoot": "projects/deployments-ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/deployments-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/deployments-ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/deployments-ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/deployments-ui/src/test.ts",
            "tsConfig": "projects/deployments-ui/tsconfig.spec.json",
            "karmaConfig": "projects/deployments-ui/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "HDSDashboardUI"
}
